{
  "name": "soap",
  "version": "0.31.0",
  "description": "A minimal node SOAP client",
  "engines": {
    "node": ">=4.0.0"
  },
  "author": "Vinay Pulim <v@pulim.com>",
  "dependencies": {
    "@types/request": "^2.48.4",
    "concat-stream": "^2.0.0",
    "debug": "^4.1.1",
    "httpntlm": "^1.5.2",
    "lodash": "^4.17.15",
    "pify": "^5.0.0",
    "request": "^2.88.2",
    "sax": "^1.2.4",
    "strip-bom": "^4.0.0",
    "uuid": "^7.0.3",
    "xml-crypto": "^1.5.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vpulim/node-soap.git"
  },
  "main": "./index.js",
  "types": "./lib/soap.d.ts",
  "directories": {
    "lib": "./lib",
    "test": "./test"
  },
  "scripts": {
    "prepare": "yarn build",
    "build": "tsc -p .",
    "clean": "rm -rf lib",
    "watch": "tsc -w -p .",
    "toc": "./node_modules/.bin/doctoc Readme.md --github --maxlevel 3",
    "cover": "nyc --extension=.ts --reporter=lcov --reporter=html --reporter=text mocha --timeout 15000 --exit test/*-test.js test/security/*.js",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js -v",
    "docs": "typedoc --out docs",
    "test": "mocha --timeout 15000 --bail --exit test/*-test.js test/security/*.js",
    "prettier": "prettier --write '**/*.ts' '**/*.js'",
    "lint": "eslint '**/*.ts'"
  },
  "keywords": [
    "soap"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/debug": "^4.1.2",
    "@types/express": "4.17.6",
    "@types/lodash": "4.14.150",
    "@types/node": "13.13.0",
    "@types/sax": "^1.0.1",
    "@types/uuid": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "2.28.0",
    "@typescript-eslint/parser": "2.28.0",
    "colors": "^1.3.3",
    "coveralls": "^3.0.5",
    "diff": "^4.0.1",
    "doctoc": "^1.4.0",
    "duplexify": "4.1.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "3.1.3",
    "express": "^4.16.4",
    "finalhandler": "^1.1.1",
    "glob": "^7.1.3",
    "jshint": "^2.10.1",
    "mocha": "^7.1.1",
    "nyc": "15.0.1",
    "prettier": "2.0.4",
    "readable-stream": "3.6.0",
    "serve-static": "^1.14.1",
    "should": "^13.2.3",
    "sinon": "9.0.2",
    "source-map-support": "^0.5.10",
    "timekeeper": "^2.1.2",
    "typedoc": "0.17.4",
    "typescript": "^3.3.3333"
  }
}
