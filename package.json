{
  "name": "soap",
  "version": "0.3.1",
  "description": "A minimal node SOAP client",
  "engines": {
    "node": ">=0.8.0"
  },
  "author": "Vinay Pulim <v@pulim.com>",
  "dependencies": {
    "node-expat": ">=1.6.1",
    "request": ">=2.9.0"
  },
  "devDependencies": {
    "jshint": "~2.4.1",
    "mocha": "~1.17.0",
    "should": "~3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/milewise/node-soap.git"
  },
  "main": "./index.js",
  "directories": {
    "lib": "./lib"
  },
  "scripts": {
    "old-test": "mocha -R spec -u exports test/*-test.js",
    "new-test": "mocha 'test/**/*.test.js'",
    "jshint":"jshint lib/*.js test/*test.js",
    "test": "npm run-script jshint && npm run-script old-test"
  },
  "keywords": [
    "soap"
  ],
  "licenses": [
    {
      "type": "MIT License",
      "url": "http://www.opensource.org/licenses/mit-license.php"
    }
  ]
}
